{
    "name": "naif/cpanel-mail",
    "description": "A Laravel Nova tool to manage Cpanel Mail to list/add/delete email addresses",
    "keywords": [
        "laravel",
        "nova",
        "email",
        "emailaddress",
        "emailaddresses",
        "email_address",
        "mail",
        "cpanel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Naif\\CpanelMail\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naif\\CpanelMail\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
